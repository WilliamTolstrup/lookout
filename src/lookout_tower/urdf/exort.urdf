<?xml version='1.0'?>
<robot name="exort">
  <!-- CHASSIS -->
  <link name="chassis">
    <inertial>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0167" ixy="0.0" ixz="0.0" iyy="0.0167" iyz="0.0" izz="0.0267"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.4 0.4 0.2"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.4 0.4 0.2"/>
      </geometry>
    </collision>
  </link>

  <!-- WHEELS -->
  <link name="front_left_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.0057" ixy="0.0" ixz="0.0" iyy="0.0057" iyz="0.0" izz="0.00072"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0"/>
      <geometry>
        <cylinder radius="0.12" length="0.08"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0"/>
      <geometry>
        <cylinder radius="0.12" length="0.08"/>
      </geometry>
    </collision>
  </link>

    <link name="front_right_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.0057" ixy="0.0" ixz="0.0" iyy="0.0057" iyz="0.0" izz="0.00072"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0"/>
      <geometry>
        <cylinder radius="0.12" length="0.08"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0"/>
      <geometry>
        <cylinder radius="0.12" length="0.08"/>
      </geometry>
    </collision>
  </link>

    <link name="back_left_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.0057" ixy="0.0" ixz="0.0" iyy="0.0057" iyz="0.0" izz="0.00072"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0"/>
      <geometry>
        <cylinder radius="0.12" length="0.08"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0"/>
      <geometry>
        <cylinder radius="0.12" length="0.08"/>
      </geometry>
    </collision>
  </link>

    <link name="back_right_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0"/>
      <mass value="0.1"/>
      <inertia ixx="0.0057" ixy="0.0" ixz="0.0" iyy="0.0057" iyz="0.0" izz="0.00072"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0"/>
      <geometry>
        <cylinder radius="0.12" length="0.08"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0"/>
      <geometry>
        <cylinder radius="0.12" length="0.08"/>
      </geometry>
    </collision>
  </link>


  <!-- JOINTS -->
  <joint name="front_left_wheel_joint" type="revolute">
    <origin xyz="0.2 0.2 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="front_left_wheel"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="1.0" velocity="1.0"/>
  </joint>

  <joint name="front_right_wheel_joint" type="revolute">
    <origin xyz="0.2 -0.2 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="front_right_wheel"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="1.0" velocity="1.0"/>
  </joint>

  <joint name="back_left_wheel_joint" type="revolute">
    <origin xyz="-0.2 0.2 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="back_left_wheel"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="1.0" velocity="1.0"/>
  </joint>

  <joint name="back_right_wheel_joint" type="revolute">
    <origin xyz="-0.2 -0.2 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="back_right_wheel"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.57" upper="1.57" effort="1.0" velocity="1.0"/>
  </joint>
</robot>